{
	"manifest_version": 2,
	"name": "Privo Secure Protocol",
	"version": "1.0",

	"description": "A secure protocol.",

	"icons": {
		"48": "icons/border-48.png"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"matches": ["privosp://*.*/*"],
			"matches": ["politics://*.*/*"],
			"js": ["privosp.js"]
		}
	]
}